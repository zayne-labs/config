{
	"name": "@zayne-labs/prettier-config",
	"type": "module",
	"version": "0.11.25",
	"description": "Zayne Labs' Prettier config",
	"author": "Ryan Zayne",
	"license": "MIT",
	"homepage": "https://github.com/zayne-labs/config#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zayne-labs/config.git"
	},
	"bugs": {
		"url": "https://github.com/zayne-labs/config/issues"
	},
	"keywords": [
		"utilities",
		"types",
		"hooks"
	],
	"sideEffects": false,
	"exports": {
		".": "./dist/index.js",
		"./plugins/*": "./dist/plugins/*.js",
		"./utils": "./dist/utils.js",
		"./constants/*": "./dist/constants/*.js"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18.x"
	},
	"scripts": {
		"build": "tsdown",
		"build:dev": "cross-env NODE_ENV=development tsdown",
		"build:test": "concurrently --prefix-colors \"yellow.bold,#7da4f8.bold,magenta\" --names PUBLINT,TSUP 'pnpm:lint:publint' 'pnpm:build:dev'",
		"dev": "pnpm build:dev --watch",
		"lint:attw": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
		"lint:eslint": "eslint . --max-warnings 0",
		"lint:format": "prettier --write .",
		"lint:publint": "publint --strict . ",
		"lint:type-check": "tsc --pretty -p tsconfig.json",
		"release": "pnpm publish --no-git-checks",
		"release:old": "changeset publish",
		"release:test": "pnpx pkg-pr-new publish",
		"test": "vitest run",
		"test:dev": "vitest dev"
	},
	"peerDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "catalog:peer",
		"prettier": "3.x.x",
		"prettier-plugin-astro": "0.x.x",
		"prettier-plugin-classnames": "0.x.x",
		"prettier-plugin-merge": "0.x.x",
		"prettier-plugin-tailwindcss": "0.x.x"
	},
	"peerDependenciesMeta": {
		"prettier-plugin-astro": {
			"optional": true
		},
		"prettier-plugin-classnames": {
			"optional": true
		},
		"prettier-plugin-merge": {
			"optional": true
		},
		"prettier-plugin-tailwindcss": {
			"optional": true
		}
	},
	"dependencies": {
		"@antfu/install-pkg": "1.1.0",
		"@clack/prompts": "1.0.0",
		"@zayne-labs/toolkit-type-helpers": "0.12.31",
		"local-pkg": "1.1.2"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "catalog:peer",
		"@total-typescript/ts-reset": "0.6.1",
		"@zayne-labs/tsconfig": "workspace:*",
		"concurrently": "9.2.1",
		"cross-env": "10.1.0",
		"prettier": "3.8.1",
		"prettier-plugin-astro": "0.14.1",
		"prettier-plugin-classnames": "0.9.0",
		"prettier-plugin-merge": "0.9.0",
		"prettier-plugin-tailwindcss": "0.7.2",
		"publint": "0.3.17",
		"tsdown": "0.20.3",
		"typescript": "5.9.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"provenance": true
	}
}
